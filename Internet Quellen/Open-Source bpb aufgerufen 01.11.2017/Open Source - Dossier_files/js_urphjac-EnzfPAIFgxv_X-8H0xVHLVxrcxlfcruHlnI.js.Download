                $.each(this.attributes, function() {
                    var txt = this.textContent || this.text,
                    name = this.localName || this.name.substr(this.name.indexOf(':')+1);
                    if (name!='id') {
                        styles[id][name] = txt;
                    }
                });
            });
        }

        // grab subtitles
        divs = $(responseObj).find("div");

        for (var i=0; i<divs.length; i++) {

