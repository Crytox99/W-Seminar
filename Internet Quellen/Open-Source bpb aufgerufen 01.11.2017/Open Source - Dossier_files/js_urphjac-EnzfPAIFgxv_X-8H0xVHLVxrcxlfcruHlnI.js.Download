(function($) {

    var defaults = {
		animationSpeed: 750,
		opacity: .5,
		imgHeigth: 160,
		imgHeigthActive: 180,
		boxWidth: 620,
		startImage: 2,
		marginTop: 20,
		marginLeft: 5,
		titleSelector: '.title',
		slideSelector: 'img',
		containerSelector: 'div'
	};

    var coverContainer;

    var imgActive;

    $.fn.coverSlider = function(options) {

        coverContainer = $(this);

        coverContainer.css('overflow', 'hidden');

        settings = $.extend({}, defaults, options);

        coverContainer.find(settings.slideSelector).css('opacity', settings.opacity);
        coverContainer.find(settings.slideSelector).click(function () {

            if (imgActive !== undefined) {
                if ($(this).attr('src') == imgActive.attr('src')) {
                    if ($(this).parent().attr('href') !== undefined) {
                        location.href = $(this).parent().attr('href');
                        return false;
                    }
                }
            }

            setImageActive($(this));

            return false;

        });

        setTimeout(timeoutImageActive, 1000);

        function timeoutImageActive() {
            setImageActive(coverContainer.find(settings.slideSelector + ':eq(' + settings.startImage + ')'));
        }

        var coverSliderIsAnimating = false;

        $('.shop-slider .topteaserright').unbind('click').click(function() {
            var allImages        = $('#coverslideshow').find('img');
            var allImagesArray   = [];
            var activeImage      = '';
            var nextImage        = '';
            var imageCount       = 0;
            var activeImageCount = 0;

            allImages.each(function() {
                var opacity = $(this).css('opacity');

                if (opacity == 1) {
                    activeImage      = $(this);
                    activeImageCount = imageCount;
                }

                allImagesArray[imageCount] = $(this);

                imageCount++;
            });

            if (typeof allImagesArray[activeImageCount + 1] !== 'undefined') {
                nextImage = allImagesArray[activeImageCount + 1];

                if (coverSliderIsAnimating === false) {
                    setImageActive(nextImage);
                }

            }
        });

        $('.shop-slider .topteaserleft').unbind('click').click(function() {
            var allImages        = $('#coverslideshow').find('img');
            var allImagesArray   = [];
            var activeImage      = '';
            var previousImage    = '';
            var imageCount       = 0;
            var activeImageCount = 0;

            allImages.each(function() {
                var opacity = $(this).css('opacity');

                if (opacity == 1) {
                    activeImage      = $(this);
                    activeImageCount = imageCount;
                }

                allImagesArray[imageCount] = $(this);

                imageCount++;
            });

            if (typeof allImagesArray[activeImageCount - 1] !== 'undefined') {
                previousImage = allImagesArray[activeImageCount - 1];

                if (coverSliderIsAnimating === false) {
                    setImageActive(previousImage);
                }

            }
        });

        function setImageActive(pImg) {
            coverSliderIsAnimating = true;

            $(settings.titleSelector).html(pImg.attr('title'));
            if (pImg.parent().attr('href') !== undefined) {
                $(settings.titleSelector).css('cursor', 'pointer');
                $(settings.titleSelector).click(function () {
                    location.href = pImg.parent().attr('href');
                    return false;
                });
            }

            var imgPosition = 0;
            if(pImg.position()) {
                imgPosition = Math.round(pImg.position().left);
            }
            var boxCenter   = settings.boxWidth / 2;
            var imageWidth  = pImg.width();
            var marginLeft  = 0 - Math.round((imgPosition + (imageWidth / 2)) - boxCenter);

            if (marginLeft > 0) {
                marginLeft = marginLeft - (settings.marginLeft * 2);
            } else {
                marginLeft = marginLeft + settings.marginLeft;
            }

            coverSliderIsAnimating = true;

            coverContainer.find(settings.containerSelector).animate({
                'margin-left': '+=' + marginLeft
